  <h1>Sign Up</h1>
  <%= form_tag '/users', method: :post do %>
    <div>Name<input type="text" name="name"></div>
    <div>Address<input type="text" name="address"></div>
    <div>City<input type="text" name="city"></div>
    <div>State<input type="text" name="state"></div>
    <div>Zip<input type="text" name="zip_code"></div>
    <div>Phone Number<input type="text" name="phone_number"></div>
    <div>Email<input type="email" name="email"></div>
    <div>Password<input type="password" name="password"></div>
    <div>Re-enter Password<input type="password" name="password_confirmation"></div>
    <div>Submit<input type="submit" value="Submit"></div>
  <% end %>